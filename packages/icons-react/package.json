{
	"name": "@pokemonle/icons-react",
	"version": "0.0.1",
	"description": "React Icons for Pokemon",
	"license": "MIT",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./icons": {
			"import": {
				"types": "./dist/icons.d.mts",
				"default": "./dist/icons.mjs"
			},
			"require": {
				"types": "./dist/icons.d.ts",
				"default": "./dist/icons.js"
			}
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "node ./scripts/build.mjs"
	},
	"devDependencies": {
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@types/react": "^18.3.12",
		"react": "^18.3.1",
		"tsup": "^8.3.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"react": ">= 18 || >= 19"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pokemonle/pokemon-icons.git",
		"directory": "packages/icons-react"
	},
	"homepage": "https://icons.pokemonle.com",
	"bugs": {
		"url": "https://github.com/pokemonle/pokemon-icons/issues"
	}
}
