{
	"name": "@pokemonle/icons-vue",
	"version": "0.0.1",
	"description": "Vue Icons for Pokemon",
	"license": "MIT",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./icons": {
			"import": {
				"types": "./dist/icons.d.mts",
				"default": "./dist/icons.mjs"
			},
			"require": {
				"types": "./dist/icons.d.ts",
				"default": "./dist/icons.js"
			}
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "node ./scripts/build.mjs"
	},
	"devDependencies": {
		"vue": "^3.4.10",
		"tsup": "^8.3.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"vue": ">= 3"
	},
	"peerDependenciesMeta": {
		"vue": {
			"optional": true
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pokemonle/pokemon-icons.git",
		"directory": "packages/icons-vue"
	},
	"homepage": "https://icons.pokemonle.com",
	"bugs": {
		"url": "https://github.com/pokemonle/pokemon-icons/issues"
	}
}
